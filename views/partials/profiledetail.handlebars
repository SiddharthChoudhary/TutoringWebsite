<article>
    {{!-- Zejie Yao --}}
    
        {{#if profile}}
        <dl>
            <dt id="firstName">First Name:</dt>
            <dd>{{profile.firstname}}</dd>
            <dt id="lastName">Last Name:</dt>
            <dd>{{profile.lastname}}</dd>
            <dt id="gender">Gender:</dt>
            <dd>{{profile.gender}}</dd>
            <dt id="subject">Subject:</dt>
            <dd>{{profile.subject}}</dd>
            <dt id="bio">Bio:</dt>
            <dd>{{profile.bio}}</dd>
  
        </dl>
        {{/if}}
    
</article>