<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Meet Your Tutor</title>
  <link href="../../vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="../../public/css/sb-admin-2.min.css" rel="stylesheet">
  <link href="../../public/css/style.css" rel="stylesheet">

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
  
   
  <link rel="stylesheet" href="../../public/css/theme2.css" type="text/css">

  <link rel="stylesheet" href="../../public/style.css"> 
   
   <script src="/socket.io/socket.io.js"></script> 
  
</head>

<body id="page-top" style="padding:0%">

  <!-- Page Wrapper -->

  <div id="wrapper">

    <!-- Sidebar -->
    <ul class="navbar-nav bg-gradient-primary primary sidebar sidebar-dark accordion" id="accordionSidebar">

      <!-- Sidebar - Brand -->
      <li class="nav-item active">
      <div class="sidebar-brand d-flex align-items-center justify-content-center side-bar-text">
        <div class="sidebar-brand-icon rotate-n-15">
          <em class="fas fa-laugh-wink"></em>
        </div>
        <div class="sidebar-brand-text mx-3">Stud-Tutor</div>
      </div>
      <!-- Divider -->
      <hr class="sidebar-divider my-0">
     </li>
      
    
      <!-- Nav Item - Dashboard -->
      <li class="nav-item active">
        <a class="nav-link" href="/dashboard">
          <em class="fas fa-fw fa-tachometer-alt"></em>
          <span>Dashboard</span></a>
          <!-- Divider -->
      <hr class="sidebar-divider">
      </li>

      

      <!-- Heading -->
      <li class="sidebar-heading side-bar-text">
        Interface
      </li>

      <!-- Nav Item - Calendar -->
      <li class="nav-item">
        <a class="nav-link side-bar-text" href="/calendar">
        <em class="far fa-fw fa-calendar-alt"></em>
          <span>Calendar</span>
        </a>
      </li>

      <!-- Nav Item - Tutors -->
      <li class="nav-item">
        <a class="nav-link side-bar-text" href="/tutors">
          <em class="fas fa-fw fa-chalkboard-teacher"></em>
          <span>Tutors</span>
        </a>
      </li>

      <!-- Nav Item - Forum -->
      <li class="nav-item">
        <a class="nav-link collapsed side-bar-text" href="#" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
          <em class="fas fa-fw fa-comments"></em>
          <span>Forum</span>
        </a>

        <div id="collapseTwo" class="collapse">
          <div class="bg-white py-2 collapse-inner rounded ">
            <h6 class="collapse-header header-text">Forum topics:</h6>
            <a class="collapse-item" href="/forum">Home</a>
              <a class="collapse-item" href="/categories/study">Study</a>
              <a class="collapse-item" href="/categories/social">Social</a>
              <a class="collapse-item" href="/categories/health">Health</a>
              <a class="collapse-item" href="/topics/new">New Topic</a>
          </div>
        </div>
        <!-- Divider -->
      <hr class="sidebar-divider">
      </li>

      <!-- Heading -->
      <li class="sidebar-heading side-bar-text">
        Learn More
      </li>

      <!-- Nav Item - Tables -->
      <li class="nav-item">
        <a class="nav-link side-bar-text" href="/about">
          <em class="fas fa-fw fa-table"></em>
          <span>About Us</span></a>
          <!-- Divider -->
      <hr class="sidebar-divider d-none d-md-block">
      </li>

      
      <li class="text-center d-none d-md-inline">
      <!-- Sidebar Toggler (Sidebar) -->
        <label hidden for="sidebarToggle">Sidebar Toggle</label>
        <button class="rounded-circle border-0" id="sidebarToggle"></button>
      
      </li>
    
    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <!-- Topbar -->
        <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

          <!-- Sidebar Toggle (Topbar) -->
          <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
            <em class="fa fa-bars"></em>
          </button>

          <!-- Topbar Search -->
          <form class="d-none d-sm-inline-block form-inline mr-auto ml-md-3 my-2 my-md-0 mw-100 navbar-search" method="post" action="/search">
            <div class="input-group">
              
              <input type="text" id="searchbtn" name="search" class="form-control bg-light border-0 small" placeholder="Search for the person" aria-label="Search">
              <div class="input-group-append">
                <label hidden for="searchbtn">Search Button</label>
                <button class="btn btn-primary" id="searchbtn" type="submit">
                  <em class="fas fa-search fa-sm"></em>
                </button>
              </div>
            </div>
          </form>
          
          <!-- Topbar Navbar -->
          <ul class="navbar-nav ml-auto">

            <!-- Nav Item - Search Dropdown (Visible Only XS) -->
            <li class="nav-item dropdown no-arrow d-sm-none">
              <a class="nav-link dropdown-toggle" href="#" id="searchDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <em class="fas fa-search fa-fw"></em>
              </a>
              <!-- Dropdown - Messages -->
              <div class="dropdown-menu dropdown-menu-right p-3 shadow animated--grow-in" aria-labelledby="searchDropdown">
                <form class="form-inline mr-auto w-100 navbar-search">
                  <div class="input-group">
                    
                    <input type="text" class="form-control bg-light border-0 small" placeholder="Search for..." aria-label="Search">
                    <div class="input-group-append">
                      <button class="btn" type="button">
                        <em class="fas fa-search fa-sm"></em>
                      </button>
                    </div>
                  </div>
                </form>
              </div>
            </li>

           <!--Nav Item- -Notification-->
            <li class="nav-item dropdown no-arrow mx-1">
              <div class="inbox">
              <div class="nav-link dropdown-toggle" id="alertsDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <em class="fas fa-bell fa-fw"></em>
                <!-- Counter - Alerts -->
                <span id="notifi" class="badge badge-danger badge-counter"></span>
              </div>

              <!-- Dropdown - Alerts -->
              <div class="dropdown-list dropdown-menu notifi-center dropdown-menu-right shadow animated--grow-in" style="width: auto !important" aria-labelledby="alertsDropdown">
                <h6 class="dropdown-header">
                  Notification
                </h6>
                <div class="notifi-container">
                
                  </div>
              </div>
              </div>
            </li>

            <!-- Nav Item - Messages -->
            <li class="nav-item dropdown no-arrow mx-1 inbox">
              <div class="inbox">
              <div class="nav-link dropdown-toggle" id="messagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <em class="fas fa-envelope fa-fw"></em>
                <!-- Counter - Messages -->
                <span id="inbox" class="badge badge-danger badge-counter"></span>
              </div>
              <!-- Dropdown - Messages -->
              <div class="dropdown-list dropdown-menu messageCenter dropdown-menu-right shadow animated--grow-in" aria-labelledby="messagesDropdown" style="width:auto !important">
                <h6 class="dropdown-header">Message Center</h6>
                <div class="inbox-container">

                  <a class="dropdown-item text-center small text-gray-500" href="/request">Read More Messages</a>
                </div>
              </div>
              </div>
            </li>

            <li class="topbar-divider d-none d-sm-block"></li>

            <!-- Nav Item - User Information -->
            <li class="nav-item dropdown no-arrow">
              <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span class="mr-2 d-none d-lg-inline text-gray-900 small">{{username}} </span>
                <img class="img-profile rounded-circle" src="https://www.w3schools.com/w3images/lights.jpg" alt="profile image">
              </a>
              <!-- Dropdown - User Information -->
              <!-- redirect to profile page -->
              <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">
                <a class="dropdown-item" href="/profile_page">
                  <em class="fas fa-user fa-sm fa-fw mr-2 text-gray-400"></em>
                  Profile
                </a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal">
                  <em class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></em>
                  Logout
                </a>
              </div>
            </li>

          </ul>

        </nav>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="mb-0 text-gray-800">{{pageHeader}}</h1>
          </div>

          <!-- Content Row -->
          {{!-- <div class="row"> --}}

            <!-- Earnings (Monthly) Card Example -->
            
          {{!-- </div> --}}

          <!-- Content Row -->

          <div class= "form-row">
          {{#if success_messages }}
            <div class="alert alert-success">{{success_messages}}</div>
          {{/if}}
          {{#if error_messages }}
          <div class="alert alert-danger">{{error_messages}}</div>
          {{/if}}
          </div>
          <div class="row">
            
         
            <!-- Area Chart -->
            
            
              {{{body}}}
                
                <!-- Card Body -->
          </div>      
        
        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <footer class="sticky-footer bg-white footer">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span>Copyright &copy; Tutoring Website 2019</span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <em class="fas fa-angle-up"></em>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">Ã—</span>
          </button>
        </div>
        <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
          <a class="btn btn-primary" href="/users/logout">Logout</a>
        </div>
      </div>
    </div>
  </div>

  <script src="../../public/javascripts/jquery/jquery.min.js"></script>
    <script src="../../public/javascripts/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="../../public/javascripts/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="../../public/javascripts/sb-admin-2.min.js"></script>
    <!-- Custom scripts for ajax inbox-->
    <script src="../../public/ajax_inbox.js"></script>
    <!-- Custom scripts for forum-->
    <script src="/index.js"></script>
    <script src="/moment.min.js"></script>
	  <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.10/handlebars.runtime.js"></script>
    <script src="/handlebarTemplates.js"></script>
    
</body>

</html>
